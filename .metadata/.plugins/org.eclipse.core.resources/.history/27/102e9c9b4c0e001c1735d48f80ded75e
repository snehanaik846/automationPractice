package package1;

import java.util.Arrays;

public class UniqueChar {
public static void Unichar(String str1) {
	char[] a=str1.toCharArray();
	Arrays.sort(a);
	for (int i=0;i<=a.length;i++) {
		for(int j=i+1;j<=a.length;j++) {
			a[i]==a[j];
		}
	}
	
	
}
	public static void main(String[] args) {
		

	}

}
