package package1;

public class SmallestLargestNoInArray {

	public static void main(String[] args) {
		int temp=0;
		Array a=[1,5,2,6,3,];
		for(int i=0;i<=a.length;i++) {
			for(int j=i+1;j<=a.length;j++) {
				if(a[i]<a[j]) {
					temp=a[i];
					a[i]=a[j];
					a[j]=temp;
				}
				continue;
			}
			
		}	}

}
