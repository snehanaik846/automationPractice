package package1;

public class anagram {

	public static boolean anagram(String str1, String str2) {
		
		if(str1.length()==str2.length()) {
			return true;
		}
		return false;
	}
	public static void main(String[] args) {
		
		String str1="Sneha";
		String str2="Senhwa";
		System.out.println(anagram(str1,str2));
	}

}
